      <xsd:element name="EventTime"  type="ce:stDoubleEventTime"          minOccurs="1" maxOccurs="1"/>
      <xsd:element name="ClockSpeed" type="ce:stEnumSimulationClockSpeed" minOccurs="1" maxOccurs="1"/>
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="ctSetSimulationTimeCmd" >
    <xsd:all>
       <xsd:element name="EventTime"      type="ce:stDoubleEventTime" minOccurs="1" maxOccurs="1"/>
       <xsd:element name="SimulationTime" type="ce:stDoubleEventTime" minOccurs="1" maxOccurs="1"/>
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>
  
  <!-- Options for SetSimPreferences
   Altitude Reference Options :   ABOVE_TERRAIN  |  ABOVE_SEA_LEVEL
   Orientation Method Options :   COUPLED_BODY_AXIS_RATES  |  UNCOUPLED_EULER_ANGLE_RATES
   Pitch Reference Options    :   RELATIVE_TO_CLIMB_ANGLE  |  RELATIVE_TO_ZERO
   Speed Mode Options         :   ACCELERATING  |  CONSTANT
   Measurement System         :   METRIC  |  ENGLISH
   -->
  <xsd:complexType name="ctSetSimPreferencesCmd" >
    <xsd:all>
       <xsd:element name="EventTime"         type="ce:stDoubleEventTime" minOccurs="1" maxOccurs="1"/>
       <xsd:element name="Altitude"          type="ce:stDoubleAltitude"  minOccurs="0" maxOccurs="1"/>
       <xsd:element name="AltitudeReference" type="xsd:string"           minOccurs="0" maxOccurs="1"/>
       <xsd:element name="MeasurementSystem" type="xsd:string"           minOccurs="0" maxOccurs="1"/>
       <xsd:element name="OrientationMethod" type="xsd:string"           minOccurs="0" maxOccurs="1"/>
       <xsd:element name="Pitch"             type="ce:ctSetRateAndAngle" minOccurs="0" maxOccurs="1"/>
       <xsd:element name="PitchReference"    type="xsd:string"           minOccurs="0" maxOccurs="1"/>   
       <xsd:element name="Roll"              type="ce:ctSetRateAndAngle" minOccurs="0" maxOccurs="1"/>
       <xsd:element name="Speed"             type="ce:stDoubleSpeed"     minOccurs="0" maxOccurs="1"/>
       <xsd:element name="SpeedMode"         type="xsd:string"           minOccurs="0" maxOccurs="1"/>
       <xsd:element name="Yaw"               type="ce:ctSetRateAndAngle" minOccurs="0" maxOccurs="1"/>                    
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="ctSignInDbCmd" >
    <xsd:all>
       <xsd:element name="EventTime"     type="ce:stDoubleEventTime" minOccurs="1" maxOccurs="1"/>
       <xsd:element name="DatabaseNames" type="ce:ctDatabaseNames"   minOccurs="1" maxOccurs="1"/>
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="ctStartPulseCollectionCmd" >
    <xsd:all minOccurs="1">
      <xsd:element name="EventTime"                 type="ce:stDoubleEventTime"       minOccurs="1" maxOccurs="1"/>
      <xsd:element name="DpmNumber"                 type="ce:stIntDpmNumber"          minOccurs="0" maxOccurs="1"/>
      <xsd:element name="DpmContentOptionsFileName" type="ce:stStringPathAndFileName" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="EgaPdwFileName"            type="ce:stStringPathAndFileName" minOccurs="0" maxOccurs="1"/>
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>

  <xsd:complexType name="ctStopPulseCollectionCmd" >
    <xsd:all minOccurs="1">
      <xsd:element name="EventTime"                 type="ce:stDoubleEventTime"       minOccurs="1" maxOccurs="1"/>
      <xsd:element name="DpmNumber"                 type="ce:stIntDpmNumber"          minOccurs="0" maxOccurs="1"/>
      <xsd:element name="DpmContentOptionsFileName" type="ce:stStringPathAndFileName" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="DpmPdwFileName"            type="ce:stStringPathAndFileName" minOccurs="0" maxOccurs="1"/>
    </xsd:all>
    <xsd:attribute name="app" type="ce:stEnumSimulation" use="required"/>
  </xsd:complexType>

</xsd:schema>
