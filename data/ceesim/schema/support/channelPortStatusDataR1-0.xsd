<xsd:schema
    targetNamespace="http://www.amherst.com/CEESIM/XML"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:ce="http://www.amherst.com/CEESIM/XML"
    attributeFormDefault="unqualified"
    elementFormDefault="qualified" >
  <!--

    Note:
   
    If a change to be made will cause the revision of this file to be incremented, then first
    create a copy of this file and rename with the new revision number.  Add a change comment
    to the new copy of the file only and introduce the new file into CM when the change is
    complete.

    Change History:

    Date        Who  Revision  Description
    2014-12-01  JSP  1.0       Original.

  -->  

  <xsd:complexType name="ctChannelPortStatus">
    <xsd:all minOccurs="1">
      <xsd:element name="ChannelNumber"                        type="ce:stIntChannelNumber"                           minOccurs="0" />
      <xsd:element name="ChannelDescription"                   type="ce:stStringChannelDescription"                   minOccurs="0" />
      <xsd:element name="Rfcu"                                 type="ce:stStringRfcu"                                 minOccurs="0" />
      <xsd:element name="Port"                                 type="ce:stStringPort"                                 minOccurs="0" />
      <xsd:element name="MinimumFrequency"                     type="ce:stDoubleMinimumFrequency"                     minOccurs="0" />
      <xsd:element name="MaximumFrequency"                     type="ce:stDoubleMaximumFrequency"                     minOccurs="0" />

      <!-- Calibrations -->

      <xsd:element name="AmplitudeShiftCharacterizationStatus" type="ce:stStringAmplitudeShiftCharacterizationStatus" minOccurs="0" />
      <xsd:element name="AmplitudeShiftCharacterizationDate"   type="ce:stDateTimeAmplitudeShiftCharacterizationDate" minOccurs="0" />
      <xsd:element name="PhaseAlignmentStatus"                 type="ce:stStringPhaseAlignmentStatus"                 minOccurs="0" />
      <xsd:element name="PhaseAlignmentDate"                   type="ce:stDateTimePhaseAlignmentDate"                 minOccurs="0" />
      <xsd:element name="PhaseLinearizationStatus"             type="ce:stStringPhaseLinearizationStatus"             minOccurs="0" />
      <xsd:element name="PhaseLinearizationDate"               type="ce:stDateTimePhaseLinearizationDate"             minOccurs="0" />
      <xsd:element name="PhaseShiftCharacterizationStatus"     type="ce:stStringPhaseShiftCharacterizationStatus"     minOccurs="0" />
      <xsd:element name="PhaseShiftCharacterizationDate"       type="ce:stDateTimePhaseShiftCharacterizationDate"     minOccurs="0" />
      <xsd:element name="ReceiveAlignmentStatus"               type="ce:stStringReceiveAlignmentStatus"               minOccurs="0" />
      <xsd:element name="ReceiveAlignmentDate"                 type="ce:stDateTimeReceiveAlignmentDate"               minOccurs="0" />
      <xsd:element name="ReceiveLinearizationStatus"           type="ce:stStringReceiveLinearizationStatus"           minOccurs="0" />
      <xsd:element name="ReceiveLinearizationDate"             type="ce:stDateTimeReceiveLinearizationDate"           minOccurs="0" />
      <xsd:element name="TdoaAlignmentStatus"                  type="ce:stStringTdoaAlignmentStatus"                  minOccurs="0" />
      <xsd:element name="TdoaAlignmentDate"                    type="ce:stDateTimeTdoaAlignmentDate"                  minOccurs="0" />
      <xsd:element name="TdoaLinearizationStatus"              type="ce:stStringTdoaLinearizationStatus"              minOccurs="0" />
      <xsd:element name="TdoaLinearizationDate"                type="ce:stDateTimeTdoaLinearizationDate"              minOccurs="0" />

      <!-- Fault Tests -->

      <xsd:element name="PhaseAccuracyStatus"                  type="ce:stStringPhaseAccuracyStatus"                  minOccurs="0" />
      <xsd:element name="PhaseAccuracyDate"                    type="ce:stDateTimePhaseAccuracyDate"                  minOccurs="0" />
      <xsd:element name="ReceiveAccuracyStatus"                type="ce:stStringReceiveAccuracyStatus"                minOccurs="0" />
      <xsd:element name="ReceiveAccuracyDate"                  type="ce:stDateTimeReceiveAccuracyDate"                minOccurs="0" />
      <xsd:element name="TdoaAccuracyStatus"                   type="ce:stStringTdoaAccuracyStatus"                   minOccurs="0" />
      <xsd:element name="TdoaAccuracyDate"                     type="ce:stDateTimeTdoaAccuracyDate"                   minOccurs="0" />

    </xsd:all>
    <xsd:attribute name="PerId" type="xsd:string" use="optional" default=""/>
  </xsd:complexType>

  <xsd:simpleType name="stDateTimeAmplitudeShiftCharacterizationDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="stDateTimePhaseAccuracyDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="stDateTimePhaseAlignmentDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="stDateTimePhaseLinearizationDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="stDateTimePhaseShiftCharacterizationDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:simpleType name="stDateTimeReceiveAccuracyDate">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

