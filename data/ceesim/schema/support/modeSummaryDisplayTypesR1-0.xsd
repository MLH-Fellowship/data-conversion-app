      //
      // Newly created types.
      //
      /////////////////////////////////////////////////////////////////////////////////////////////// -->

  <xsd:simpleType name="xtIntEmitterNumber">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue>1</DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtIntPlatformNumber">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue>1</DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="32767"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtStringEmitterId">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="5"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtStringEmitterName">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="0"/>
      <xsd:maxLength value="22"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtStringModeName">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue>mode</DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="22"/>
    </xsd:restriction>
  </xsd:simpleType>

<!-- ///////////////////////////////////////////////////////////////////////////////////////////////
     //
     // Emitter Mode Summary  Extensions
     //
     //    Additions: Emitter Number, Name, Id, Mode Name
     //
     /////////////////////////////////////////////////////////////////////////////////////////////// -->

  <xsd:complexType name="xtEmitterModeSummary">
      <xsd:complexContent>
         <xsd:extension base="ce:ctEmitterModeSummary">
            <xsd:all>
               <xsd:element name="EmitterNumber"  type="ce:xtIntEmitterNumber"  default="1" />
               <xsd:element name="EmitterName"    type="ce:xtStringEmitterName" default="1" />
               <xsd:element name="EmitterId"      type="ce:xtStringEmitterId"   default="1" />
               <xsd:element name="PlatformNumber" type="ce:xtIntPlatformNumber" default="1" />
               <xsd:element name="ModeName"       type="ce:xtStringModeName"    default="1" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>


