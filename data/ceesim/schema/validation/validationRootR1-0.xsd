      <xsd:element name ="AmopResolution"         type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="BurstSupported"         type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ChannelSourceType"      type="ce:stEnumChannelSourceType"     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="FmopMaxDev"             type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="CurrentSourceState"     type="ce:stEnumSourceState"           minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Description"            type="xsd:string"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="DualRiseTime"           type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ExternalRadar"          type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="HfimFreqList"           type="ce:ctFrequencyRangeList"        minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="HfimList"               type="ce:ctHfimPatternLookupData"     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="HighFreq"               type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="InactiveFreqList"       type="ce:ctFrequencyRangeList"        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="LowFreq"                type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MdfAvailable"           type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MdfMaster"              type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MdfMode"                type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MinChirpRate"           type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MinFreqRes"             type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Number"                 type="xsd:int"                        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="PhaseShiftMode"         type="ce:stEnumBiQuadPhaseMode"       minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="PmopList"               type="ce:ctPmopBandDataList"          minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Pooled"                 type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimAgilitySupported"   type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimChannelSource"      type="ce:stEnumRfguSourceType"        minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFmopList"           type="ce:ctChirpFmopSourceDataList"   minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFmopTable"          type="ce:ctChirpFmopTable"            minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFmopMaxDev"         type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFmopSupported"      type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimDigitalFreqRes"     type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFineFreqSupported"  type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimFreqRes"            type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimHfimSupported"      type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimHsAmopPresent"      type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimPeriodModSupported" type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimPmopSupported"      type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimSubbandFreqList"    type="ce:ctSubbandFrequencyDataList"  minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="PrimTuningState"        type="ce:stEnumTuningState"           minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecAgilitySupported"    type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecChannelSource"       type="ce:stEnumRfguSourceType"        minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFmopList"            type="ce:ctChirpFmopSourceDataList"   minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFmopTable"           type="ce:ctChirpFmopTable"            minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFmopMaxDev"          type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFmopSupported"       type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecDigitalFreqRes"      type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFineFreqSupported"   type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecFreqRes"             type="xsd:double"                     minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecHfimSupported"       type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecHsAmopPresent"       type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecPeriodModSupported"  type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecPmopSupported"       type="xsd:boolean"                    minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecSubbandFreqList"     type="ce:ctSubbandFrequencyDataList"  minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SecTuningState"         type="ce:stEnumTuningState"           minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="ProgramSimFreqDisplay"  type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="RfAttenType"            type="ce:stEnumRfAttenuatorType"      minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="RfguType"               type="ce:stEnumRfguType"              minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="RiseTime"               type="ce:stEnumRiseTimeState"         minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="RsiChannel"             type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SfaPresent"             type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SfaStartFreqList"       type="ce:ctValidationEnum"            minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="SfaStopFreqList"        type="ce:ctValidationEnum"            minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="ShadowTime"             type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SourceState"            type="ce:stEnumSourceState"           minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SourceType"             type="ce:stEnumChannelSourceType"     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="UseFreqBandDesignators" type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="VideoAoaModeEnabled"    type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpBandDataList" >
    <xsd:sequence>
      <xsd:element name ="ChirpBandData" type="ce:ctChirpBandData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpBandData" >
    <xsd:sequence>
      <xsd:element name ="MaxVolts"          type="xsd:double"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Scale"             type="xsd:int"                       minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ChirpVoltDataList" type="ce:ctChirpVoltageBandDataList" minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpFmopSourceDataList" >
    <xsd:sequence>
      <xsd:element name ="ChirpFmopSourceData" type="ce:ctChirpFmopSourceData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpFmopSourceData" >
    <xsd:sequence>
      <xsd:element name ="Direction" type="ce:stEnumChirpModulationDirection" minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MaxDev"    type="xsd:double"                        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Modsense"  type="xsd:double"                        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Start"     type="xsd:double"                        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Stop"      type="xsd:double"                        minOccurs="1" maxOccurs="1"></xsd:element>   
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpFmopTable" >
    <xsd:sequence>
      <xsd:element name ="MaxVolt"           type="xsd:double"             minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MaxVoltForRate"    type="xsd:double"             minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="MaxVoltPercent"    type="xsd:double"             minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ChirpBandDataList" type="ce:ctChirpBandDataList" minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="FmopBandDataList"  type="ce:ctFmopBandDataList"  minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpVoltageBandDataList" >
    <xsd:sequence>
      <xsd:element name ="ChirpVoltageBandData" type="ce:ctChirpVoltageBandData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChirpVoltageBandData" >
    <xsd:sequence>
      <xsd:element name ="MinRate" type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
      <xsd:element name ="MaxRate" type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
      <xsd:element name ="Msb"     type="xsd:int"    minOccurs="1"  maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctFmopBandDataList" >
    <xsd:sequence>
      <xsd:element name ="FmopBandData" type="ce:ctFmopBandData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctFmopBandData" >
    <xsd:sequence>
      <xsd:element name ="AbsMaxVolt" type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
      <xsd:element name ="Scale"      type="xsd:int"    minOccurs="1"  maxOccurs="1"></xsd:element>
      <xsd:element name ="VoltLsb"    type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctFrequencyRangeList" >
    <xsd:sequence>
      <xsd:element name ="FrequencyRange" type="ce:ctFrequencyRange"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctFrequencyRange" >
    <xsd:sequence>
      <xsd:element name ="Start" type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
      <xsd:element name ="Stop"  type="xsd:double" minOccurs="1"  maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctGpValidationDataList" >
    <xsd:sequence>
      <xsd:element name ="GpValidationData" type="ce:ctGpValidationData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctGpValidationData" >
    <xsd:sequence>
      <xsd:element name ="Pool" type="xsd:int" minOccurs="1"  maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctHfimModulationPatternList" >
    <xsd:sequence>
      <xsd:element name ="HfimModulationPattern" type="ce:ctHfimModulationPattern"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctHfimModulationPattern" >
    <xsd:sequence>
      <xsd:element name ="Comment"      type="xsd:string"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Duration"     type="xsd:double"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Name"         type="xsd:string"                     minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Period"       type="ce:stEnumHfimSamplePeriodKind"  minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="OriginalAwg"  type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Valid"        type="xsd:boolean"                    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ViewMode"     type="ce:stEnumHfimViewMode"          minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctHfimPatternLookupDataList" >
    <xsd:sequence>
      <xsd:element name ="hfimPatternLookupData" type="ce:ctHfimPatternLookupData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctHfimPatternLookupData" >
    <xsd:sequence>
      <xsd:element name ="ChannelSourceStateList" type="ce:ctChannelIdAndSourceStateList" minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="DownloadPatternList"    type="ce:ctValidationEnum"              minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Duration"               type="xsd:double"                       minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="FilterPath"             type="ce:stEnumHfimFilterPathSelection" minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ICrc"                   type="xsd:int"                          minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="IqCombine"              type="xsd:boolean"                      minOccurs="1" maxOccurs="1"></xsd:element>
