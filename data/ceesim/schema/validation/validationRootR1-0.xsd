<xsd:schema
    targetNamespace="http://www.amherst.com/CEESIM/XML"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:ce="http://www.amherst.com/CEESIM/XML"
    attributeFormDefault="unqualified"
    elementFormDefault="qualified"
>
  <!--
    Note:
    
    If a change to be made will cause the revision of this file to be incremented, then first
    create a copy of this file and rename with the new revision number.  Add a change comment
    to the new copy of the file only and introduce the new file into CM when the change is
    complete.
    
    Change History:
    
    Date        Who  Revision  Description
    2013-06-17  XXX  1.0       Original.
  -->
  
  <!-- The major revision needs to be a single value, i.e., minInclusive == maxInclusive -->
  <xsd:simpleType name="stValidationMajorRevision">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="1"/>
      <xsd:maxInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <!-- The minor revision ranges from 0 to the current minor revision -->
  <xsd:simpleType name="stValidationMinorRevision">
    <xsd:restriction base="xsd:int">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:element name ="CeesimValidationData" type="ce:ctValidationSection"/>

  <xsd:complexType name ="ctValidationSection">
    <xsd:sequence>
      <xsd:element name ="ValidationSection" type="ce:ctValidationRecord" minOccurs="1" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Major" type="ce:stValidationMajorRevision" use="required"/>
    <xsd:attribute name="Minor" type="ce:stValidationMinorRevision" use="required"/>
    <xsd:attribute name="PerId" type="xsd:string" use="optional" default=""/>
  </xsd:complexType>

  <xsd:complexType name ="ctValidationRecord" >
    <xsd:choice minOccurs="1" maxOccurs="unbounded">
      <xsd:element name ="Int"                  type="ce:ctValidationInteger"                minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Dou"                  type="ce:ctValidationDouble"                 minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Enum"                 type="ce:ctValidationEnum"                   minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="Bool"                 type="ce:ctValidationBoolean"                minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ChPwrFreqDataList"    type="ce:ctChannelPowerFrequencyDataList"    minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ChValidationList"     type="ce:ctChannelValidationDataList"        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="GpValidationList"     type="ce:ctGpValidationDataList"             minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="HfimPatternList"      type="ce:ctHfimModulationPatternList"        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="ModPatternList"       type="ce:ctModulationPatternRecordList"      minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="PoolValidationList"   type="ce:ctPoolValidationDataList"           minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SystemType"           type="ce:stEnumSystemType"                   minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="TxPatternList"        type="ce:ctTransmitPatternRecordList"        minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="UniPolarizationList"  type="ce:ctUniformPolarizationRecordList"    minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:choice>
    <xsd:attribute name="N" type="xsd:string"/>
  </xsd:complexType>

<!-- /////////////////////////////////////////////////////////////////////////////////
     //
     // Complex Type Validation Type Definitions
     //
     ///////////////////////////////////////////////////////////////////////////// -->

  <xsd:complexType name ="ctValidationBoolean" >
    <xsd:all>
      <xsd:element name ="Def" type="xsd:boolean" minOccurs="0" maxOccurs="1"></xsd:element>
    </xsd:all>
    <xsd:attribute name="N" type="xsd:string" />
    <xsd:attribute name="Const" type="xsd:boolean" use="optional" default="false"/>
  </xsd:complexType>

  <xsd:complexType name ="ctValidationDouble" >
    <xsd:all>
      <xsd:element name ="Def" type="xsd:double" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Min" type="xsd:double" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Max" type="xsd:double" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Res" type="xsd:double" minOccurs="0" maxOccurs="1"></xsd:element>
    </xsd:all>
    <xsd:attribute name="N" type="xsd:string" />
    <xsd:attribute name="Const" type="xsd:boolean" use="optional" default="false"/>
  </xsd:complexType>

  <xsd:complexType name ="ctValidationEnum" >
    <xsd:sequence>
      <xsd:element name ="Def" type="xsd:string" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Opt" type="ce:ctValidationIndexedString" minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
    <xsd:attribute name="N"     type="xsd:string"  use="optional" />
    <xsd:attribute name="Const" type="xsd:boolean" use="optional" default="false"/>
  </xsd:complexType>

  <xsd:complexType name ="ctValidationInteger" >
    <xsd:all>
      <xsd:element name ="Def" type="xsd:int" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Min" type="xsd:int" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Max" type="xsd:int" minOccurs="0" maxOccurs="1"></xsd:element>
      <xsd:element name ="Res" type="xsd:int" minOccurs="0" maxOccurs="1"></xsd:element>
    </xsd:all>
    <xsd:attribute name="N"     type="xsd:string" />
    <xsd:attribute name="Const" type="xsd:boolean" use="optional" default="false"/>
  </xsd:complexType>

  <xsd:complexType name ="ctValidationIndexedString" >
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="Ind" type="xsd:int" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>


<!-- /////////////////////////////////////////////////////////////////////////////////
     //
     // Complex Type Ada Record Definitions
     //
     ///////////////////////////////////////////////////////////////////////////// -->

  <xsd:complexType name ="ctChannelIdAndSourceStateList" >
    <xsd:sequence>
      <xsd:element name ="ChannelIdAndSourceState" type="ce:ctChannelIdAndSourceState"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChannelIdAndSourceState" >
    <xsd:sequence>
      <xsd:element name ="Id"          type="xsd:int"              minOccurs="1" maxOccurs="1"></xsd:element>
      <xsd:element name ="SourceState" type="ce:stEnumSourceState" minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChannelPowerFrequencyDataList" >
    <xsd:sequence>
      <xsd:element name ="ChannelPowerFrequencyData" type="ce:ctChannelPowerFrequencyData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChannelPowerFrequencyData" >
    <xsd:sequence>
      <xsd:element name ="Channel"       type="xsd:int"                 minOccurs="1"   maxOccurs="1"></xsd:element>
      <xsd:element name ="PowerFreqList" type="ce:ctPowerFrequencyData" minOccurs="1" maxOccurs="1"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChannelValidationDataList" >
    <xsd:sequence>
      <xsd:element name ="ChannelValidationData" type="ce:ctChannelValidationData"  minOccurs="0" maxOccurs="unbounded"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name ="ctChannelValidationData" >
