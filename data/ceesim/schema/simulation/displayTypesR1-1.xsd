  <xsd:simpleType name="dtIntDwellTimePulses">
    <xsd:annotation>
      <xsd:appinfo>
         <DisplayUnit>Pulses</DisplayUnit>
         <TranslationEntryList>
          <TranslationEntry EntryType="FrequencySequence" EntryName="DwellTimePulses">
            <Column>10</Column>
            <Editable>true</Editable>
            <DisplayName>Dwell Pulse</DisplayName>
            <Group>Advanced</Group>
          </TranslationEntry>
          <TranslationEntry EntryType="PriSequence" EntryName="DwellTimePulses">
            <Column>6</Column>
            <Editable>true</Editable>
            <DisplayName>Dwell Pulse</DisplayName>
            <Group>Standard</Group>
          </TranslationEntry>
          <TranslationEntry EntryType="PulseSequence" EntryName="DwellTimePulses">
            <Column>8</Column>
            <Editable>true</Editable>
            <DisplayName>Dwell Pulse</DisplayName>
            <Group>Advanced</Group>
          </TranslationEntry>
        </TranslationEntryList>
      </xsd:appinfo>
      <xsd:documentation>Number of pulses in the dwell. Valid option if DwellKind = PulseBased and, if present in complex type, DwellStatus = true.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ce:stIntDwellTimePulses">
    </xsd:restriction>
  </xsd:simpleType>

