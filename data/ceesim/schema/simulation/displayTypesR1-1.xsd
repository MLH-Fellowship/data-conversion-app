<!-- ///////////////////////////////////////////////////////////////////////////////////////////////
     //
     // Scan Extensions
     //
     //    Additions:
     //       - Each segment will have a segment type, Az Transmit pattern, and El Transmit pattern.
     //       - AzEl, Electronic, Helical, and non-interleaved raster will have ScanPeriodDerived.
     //
     /////////////////////////////////////////////////////////////////////////////////////////////// -->

   <xsd:complexType name="xtScanSegmentAzEl">
      <xsd:complexContent>
         <xsd:extension base="ce:ctScanSegmentAzEl">
            <xsd:all>
               <xsd:element name="AzTransmitPattern" type="ce:xtEnumAzTransmitPattern"/>
