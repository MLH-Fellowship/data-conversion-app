  <xsd:simpleType name="xtStringSegmentType">
      <xsd:annotation>
         <xsd:appinfo>
            <DefaultValue></DefaultValue>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string"></xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtEnumAzTransmitPattern">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtEnumChannelCode">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtEnumModulationType">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="None"/>
      <xsd:enumeration value="Interpulse"/>
      <xsd:enumeration value="Intrapulse"/>
      <xsd:enumeration value="PulseGroup"/>
      <xsd:enumeration value="IntrapulsePulseGroup"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtDoubleDwellDuration">
      <xsd:annotation>
         <xsd:appinfo>
            <DefaultValue>0.0</DefaultValue>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:double">
         <!-- These values are in SI Base Units, seconds for this field -->
         <xsd:minInclusive value="0"/>
         <xsd:maxInclusive value="18446744073709.551615"/>
      </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtDoubleScanPeriodDerived">
      <xsd:annotation>
         <xsd:appinfo>
            <DefaultValue>0.0</DefaultValue>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:double">
         <xsd:minInclusive value="0"/>
         <xsd:maxInclusive value="18446744073709.551615"/>
      </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtEnumElTransmitPattern">
    <xsd:annotation>
      <xsd:appinfo>
        <DefaultValue></DefaultValue>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value=""/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="xtDoubleTotalDwell">
      <xsd:annotation>
         <xsd:appinfo>
            <DefaultValue></DefaultValue>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:double">
         <xsd:minInclusive value="0"/>
         <xsd:maxInclusive value="4294967295.0"/>
      </xsd:restriction>
  </xsd:simpleType>

<!-- ///////////////////////////////////////////////////////////////////////////////////////////////
     //
     // PRI Extensions
     //
     //    Additions:
     //       - Each segment will have a segment type field.
     //       - Subsegments do not have a DwellStatus element.  To be consistent with the other segments
     //         a DwellStatus element will be created for subsegments.  This will simplify the validation
     //         dependency XML file as special cases do not have to be written for subsegments.
     //
     /////////////////////////////////////////////////////////////////////////////////////////////// -->

  <xsd:complexType name="xtPriSegmentSingle">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentSingle">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Single" />
               <xsd:element name="PriJitter" type="ce:stBoolJitterStatus" default="false"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtPriSegmentDwell">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentDwell">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Dwell" />
               <xsd:element name="PriJitter" type="ce:stBoolJitterStatus" default="false"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtPriSegmentPeriodic">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentPeriodic">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Periodic" />
               <xsd:element name="PriJitter" type="ce:stBoolJitterStatus" default="false"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtPriSegmentStaggered">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentStaggered">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Stagger" />
               <xsd:element name="DwellDuration" type="ce:xtDoubleDwellDuration" default="0.0" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtStaggeredSeriesPriSegment">
      <xsd:complexContent>
         <xsd:extension base="ce:ctStaggeredSeriesPriSegment">
            <xsd:all>
               <xsd:element name="DwellStatus" type="ce:stBoolDwellStatus" default="true"/>
               <xsd:element name="DwellDuration" type="ce:xtDoubleDwellDuration" default="0.0" />
               <xsd:element name="PriJitter" type="ce:stBoolJitterStatus" default="false"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtPriSegmentDiscreteJitter">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentDiscreteJitter">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Discrete Jitter" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtDiscreteJitterSeriesPriSegment">
      <xsd:complexContent>
         <xsd:extension base="ce:ctDiscreteJitterSeriesPriSegment">
            <xsd:all>
               <xsd:element name="DwellStatus" type="ce:stBoolDwellStatus" default="true"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtPriSegmentCw">
      <xsd:complexContent>
         <xsd:extension base="ce:ctPriSegmentCw">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="CW" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

<!-- ///////////////////////////////////////////////////////////////////////////////////////////////
     //
     // Frequency Extensions
     //
     //    Additions:
     //       - Each segment will have a segment type field.
     //       - Subsegments do not have a DwellStatus element.  To be consistent with the other segments
     //         a DwellStatus element will be created for subsegments.  This will simplify the validation
     //         dependency XML file as special cases do not have to be written for subsegments.
     //
     /////////////////////////////////////////////////////////////////////////////////////////////// -->

   <xsd:complexType name="xtFrequencySegmentAssociated">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentAssociated">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Associated" />
               <xsd:element name="ChannelCode" type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtFrequencySegmentSingle">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentSingle">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Single" />
               <xsd:element name="ChannelCode" type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtFrequencySegmentDwell">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentDwell">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Dwell" />
               <xsd:element name="ChannelCode" type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtFrequencySegmentPeriodic">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentPeriodic">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Periodic" />
               <xsd:element name="ChannelCode" type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtFrequencySegmentSeries">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentSeries">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Series" />
               <!-- This additional field is intentionally named the same as the PRI ones, even
                    though it has a different type.  The functions used to calculate the values
                    and initialize the fields are the same for both sequences -->
               <xsd:element name="DwellDuration" type="ce:xtDoubleTotalDwell" default="" /> 
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtSeriesFrequencySegment">
      <xsd:complexContent>
         <xsd:extension base="ce:ctSeriesFrequencySegment">
            <xsd:all>
               <xsd:element name="DwellStatus"  type="ce:stBoolDwellStatus" default="true"/>
               <xsd:element name="ChannelCode"  type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtFrequencySegmentDiscreteAgility">
      <xsd:complexContent>
         <xsd:extension base="ce:ctFrequencySegmentDiscreteAgility">
            <xsd:all>
               <xsd:element name="SegmentType" type="ce:xtStringSegmentType" default="Discrete Agility" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtDiscreteAgilityFrequencySegment">
      <xsd:complexContent>
         <xsd:extension base="ce:ctDiscreteAgilityFrequencySegment">
            <xsd:all>
               <xsd:element name="DwellStatus" type="ce:stBoolDwellStatus" default="true" />
               <xsd:element name="ChannelCode" type="ce:xtEnumChannelCode"/>
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

<!-- ///////////////////////////////////////////////////////////////////////////////////////////////
