               <xsd:element name="SegmentType"       type="ce:xtStringSegmentType" default="Conical" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

   <xsd:complexType name="xtScanSegmentElectronic">
