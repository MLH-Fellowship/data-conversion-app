               <xsd:element name="ScanPeriodDerived" type="ce:xtDoubleScanPeriodDerived" />
               <xsd:element name="SegmentType"       type="ce:xtStringSegmentType" default="Az El" />
            </xsd:all>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>

