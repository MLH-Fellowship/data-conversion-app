  <!--
    Note:
    
    If a change to be made will cause the revision of this file to be incremented, then first
    create a copy of this file and rename with the new revision number.  Add a change comment
    to the new copy of the file only and introduce the new file into CM when the change is
    complete.
    
    Change History:
    
    Date        Who  Revision  Description
    2013-03-11  XXX  1.0       Original.
    2013-03-22  JSP  1.0       Remove ModeTransitionPeriod and InterruptDurationOverride fields.
                               These fields will be moved to the rev 1.1 schema.
			       Add key and unique constraints.
                               
  -->
  
  <xsd:include schemaLocation="commonSimpleTypesR1-0.xsd"/>

  <xsd:complexType name="ctEmitterMode">
    <xsd:all minOccurs="1">
      <xsd:element name="EmitterModeHeader" type="ce:ctEmitterModeHeader" minOccurs="1"/>
      <xsd:element name="Generators" type="ce:ctGeneratorList" minOccurs="0">
        <xsd:unique name="uniqueGeneratorNumber">
          <xsd:selector xpath="ce:Generator"/>
          <xsd:field xpath="ce:GeneratorNumber"/>
        </xsd:unique>
      </xsd:element>
      <xsd:element name="EmitterSubmodes" type="ce:ctEmitterSubmodeList" minOccurs="0">
        <xsd:key name="keyEmitterSubmodeId">
          <xsd:selector xpath="ce:EmitterSubmode/ce:EmitterSubmodeHeader"/>
          <xsd:field xpath="ce:SubmodeId"/>
        </xsd:key>
        <xsd:key name="keyEmitterSubmodeName">
          <xsd:selector xpath="ce:EmitterSubmode/ce:EmitterSubmodeHeader"/>
          <xsd:field xpath="ce:SubmodeName"/>
        </xsd:key>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="PerId" type="xsd:string" use="optional" default=""/>
  </xsd:complexType>

  <xsd:complexType name="ctEmitterModeHeader">
    <xsd:all minOccurs="1">
      <xsd:element name="ModeName"                  type="ce:stStringModeName"            minOccurs="1"/>
      <xsd:element name="LastUpdateDate"            type="ce:stDateModificationDate"      minOccurs="0"/>
      <xsd:element name="ModeComment"               type="ce:stStringModeComment"         minOccurs="0"/>
      <xsd:element name="ModeProtectionSwitch"      type="ce:stBoolModeProtectionSwitch"  minOccurs="0"/>
      <xsd:element name="SensitivityOverride"       type="ce:stBoolSensitivityOverride"   minOccurs="0"/>
      <xsd:element name="PriorityKind"              type="ce:stEnumPriority"              minOccurs="0"/>
      <xsd:element name="GpAssigned"                type="ce:stBoolGpAssigned"            minOccurs="0"/>
      <xsd:element name="GpPooled"                  type="ce:stBoolGpPooled"              minOccurs="0"/>
      <xsd:element name="GpOrPoolNumber"            type="ce:stIntGpOrPoolNumber"         minOccurs="0"/>
      <xsd:element name="JammerStatus"              type="ce:stBoolJammerStatus"          minOccurs="0"/>
      <xsd:element name="AzimuthTestPoint"          type="ce:stBoolAzimuthTestPoint"      minOccurs="0"/>
      <xsd:element name="ElevationTestPoint"        type="ce:stBoolElevationTestPoint"    minOccurs="0"/>
      <xsd:element name="PriTestPoint"              type="ce:stBoolPriTestPoint"          minOccurs="0"/>
      <xsd:element name="PriTestPoint2"             type="ce:stBoolPriTestPoint2"         minOccurs="0"/>
      <xsd:element name="PriTestPoint3"             type="ce:stBoolPriTestPoint3"         minOccurs="0"/>
      <xsd:element name="PriTestPoint4"             type="ce:stBoolPriTestPoint4"         minOccurs="0"/>
      <xsd:element name="DigitalVideoTestPoint"     type="ce:stBoolDigitalVideoTestPoint" minOccurs="0"/>
      <xsd:element name="ChannelVideoTestPoint"     type="ce:stBoolChannelVideoTestPoint" minOccurs="0"/>
      <xsd:element name="ChannelAudioTestPoint"     type="ce:stBoolChannelAudioTestPoint" minOccurs="0"/>
      <xsd:element name="RfBlankingVideoTestPoint"  type="ce:stBoolRfBlankingVideoTestPoint" minOccurs="0"/>
      <xsd:element name="SiteId"                    type="ce:stIntSiteId"                     minOccurs="0"/>
      <xsd:element name="VirtualNumber"             type="ce:stIntVirtualNumber"              minOccurs="0"/>
      <xsd:element name="ExternalModeId"            type="ce:stIntExternalModeId"             minOccurs="0"/>
      <xsd:element name="PgpDefaultMode"            type="ce:stBoolPgpDefaultMode"            minOccurs="0"/>
      <xsd:element name="PgpDropTrackMode"          type="ce:stBoolPgpDropTrackMode"          minOccurs="0"/>
    </xsd:all>
  </xsd:complexType>

  <xsd:complexType name="ctGeneratorList">
    <xsd:sequence minOccurs="0" maxOccurs="32">
      <xsd:element name="Generator" type="ce:ctGenerator">
        <xsd:unique name="uniquePriSegmentNumber">
          <xsd:selector xpath="ce:PriSequence/*"/>
          <xsd:field xpath="ce:SegmentNumber"/>
        </xsd:unique>
        <xsd:unique name="uniqueFrequencySegmentNumber">
          <xsd:selector xpath="ce:FrequencySequence/*"/>
          <xsd:field xpath="ce:SegmentNumber"/>
        </xsd:unique>
        <xsd:unique name="uniqueScanSegmentNumber">
          <xsd:selector xpath="ce:ScanSequence/*"/>
          <xsd:field xpath="ce:SegmentNumber"/>
        </xsd:unique>
        <xsd:unique name="uniquePulseSegmentNumber">
          <xsd:selector xpath="ce:PulseSequence/*"/>
          <xsd:field xpath="ce:SegmentNumber"/>
        </xsd:unique>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="ctEmitterSubmodeList">
    <xsd:sequence minOccurs="0" maxOccurs="unbounded">
      <xsd:element name="EmitterSubmode" type="ce:ctEmitterSubmode"/>
    </xsd:sequence>
  </xsd:complexType>

